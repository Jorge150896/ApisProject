{
  "env": {
    "browser": true,
    "es2021": true
  },

  "extends": ["react-app", "react-app/jest"],

  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "react-hooks", "prettier"],

  // }
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "error"
  }
}
